{
  "name": "snack-theme-clean",
  "version": "0.1.0",
  "description": "Theme for the static site generator snack",
  "private": true,
  "repository": "https://github.com/webhunger-ch/snack-theme-clean",
  "author": "henning@webhunger.ch",
  "scripts": {
    "build:js": "webpack --config webpack.config.js",
    "watch:js": "webpack --config webpack.config.js --watch",
    "build:css": "node-sass src/scss -o assets/css/ --output-style compressed",
    "watch:css": "node-sass src/scss -o assets/css/ -w --output-style compressed"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "aos": "^2.3.4",
    "node-sass": "^7.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
